{
  "name": "@tanstack/react-template",
  "version": "0.0.0",
  "description": "React adapter for TanStack Template",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/template.git",
    "directory": "packages/react-template"
  },
  "homepage": "https://tanstack.com/template",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "keywords": [
    "react",
    "tanstack",
    "template"
  ],
  "scripts": {
    "clean": "premove ./build ./dist",
    "test:eslint": "eslint ./src",
    "test:lib": "vitest --passWithNoTests",
    "test:lib:dev": "pnpm test:lib --watch",
    "test:types": "tsc",
    "build": "tsdown"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@tanstack/template": "workspace:*",
    "@tanstack/react-store": "^0.8.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.5.7",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.2.8",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "react": "^19.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
